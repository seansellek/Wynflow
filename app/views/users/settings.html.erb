<h1>Settings</h1>
<% if flash[:notice] %>
<div class="error">
  <%= flash[:notice] %>
</div>
<% end %>
<%= form_for @user do |f| %>

<p>
  <%= f.label :full_name %><br>
  <%= f.text_field :full_name %>
</p>

<p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>



<p>
  <%= f.submit %>
</p>
<% end %>