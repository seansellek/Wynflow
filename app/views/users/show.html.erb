


<h1>Hi, <%= @user.full_name %>!</h1>
  <%= link_to 'New Project', new_project_path %>
<% @user.projects.each do |project| %>
<div class="project">
  <div class="score">
    <span><%= project.votes || 0 %></span>
    <%= link_to 'Upvote', project_path(project) + '/vote' %>
  </div>
  <div class='img'>
    <img src="<%= project.img %>" alt="" />
  </div>
  <a href="<%=project_path(project)%>"><h3><%= project.title %></h3></a>

  <p>
    <%= project.description %>
  </p>

  <div class="links">
  <%=link_to "Visit App", project.url, class: 'button' %>
  <%=link_to "See Source", project.repo, class: 'button' %>
    <%= link_to('edit', edit_project_path(project), class: 'button') %>
    <%= link_to('destroy', project_path(project), method: :delete, class: 'button') %>
  </div>


</div>
<% end %>
