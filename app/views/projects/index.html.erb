    <% @projects.order(votes: :desc).each do |project| %>

      <div class="project">
        <a href="<%=project_path(project)%>"><h3><%= project.title %></h3></a>

        <p>
          <%= project.description %>
        </p>
        <div class="score">
          <%= project.votes %>
          <%= link_to 'Upvote', project_path(project) + '/vote' %>
        </div>
        <div class="date_created">
        <% project.created_at %>
        <% link_to 'CreateDate', project_path(project) +'/date' %>
      </div>
      <% end %>
